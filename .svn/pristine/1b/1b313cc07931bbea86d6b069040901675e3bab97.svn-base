package com.hnjk.edu.teaching.helper;

import java.util.Comparator;

import com.hnjk.edu.teaching.model.ExamSettingDetails;
/**
 * 考试计划设置明细排序<code>ComparatorExamSettingDetails</code><p>
 * @author： 广东学苑教育发展有限公司
 * @since： 2011-9-15 下午03:21:43
 * @see 
 * @version 1.0
*/
public class ComparatorExamSettingDetails implements Comparator<ExamSettingDetails>{

	@Override
	public int compare(ExamSettingDetails o1, ExamSettingDetails o2) {
		int flag=o1.getCourseName().compareTo(o2.getCourseName());
		return flag;

	}
}