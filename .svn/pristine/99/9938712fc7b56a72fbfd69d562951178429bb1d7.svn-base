package com.hnjk.edu.teaching.jaxb.entity;

import javax.xml.bind.annotation.XmlAttribute;



/**
 * 
 * 课程提要类
 * @author luof 
 */
public class AchievementCourseTip
{

    private String  courseguid ;
    private String  namechs;
    private Long    studyhour ;
    private Integer itemnum ;
    private String  teachingtimestylecode ;
    private String  teachingtimestyle;
    private String  startdate ;
    private String  enddate ;

    public AchievementCourseTip()
    {
    	
    }

    public AchievementCourseTip(String courseguid, String namechs,
            Long studyhour, Integer itemnum, String teachingtimestylecode,
            String teachingtimestyle, String startdate, String enddate)
    {
        this.courseguid = courseguid;
        this.namechs = namechs;
        this.studyhour = studyhour;
        this.itemnum = itemnum;
        this.teachingtimestyle = teachingtimestyle;
        this.teachingtimestylecode = teachingtimestylecode;
        this.startdate = startdate;
        this.enddate = enddate;
    }
    
    @XmlAttribute
    public String getCourseguid()
    {
        return courseguid;
    }

    public void setCourseguid(String courseguid)
    {
        this.courseguid = courseguid;
    }
    
    @XmlAttribute
    public Long getStudyhour()
    {
        return studyhour;
    }

    public void setStudyhour(Long studyhour)
    {
        this.studyhour = studyhour;
    }
    
    @XmlAttribute
    public Integer getItemnum()
    {
        return itemnum;
    }

    public void setItemnum(Integer itemnum)
    {
        this.itemnum = itemnum;
    }
    
    @XmlAttribute
    public String getNamechs()
    {
        return namechs;
    }

    public void setNamechs(String namechs)
    {
        this.namechs = namechs;
    }
    
    @XmlAttribute
    public String getTeachingtimestyle()
    {
        return teachingtimestyle;
    }
    
    public void setTeachingtimestyle(String teachingtimestyle)
    {
        this.teachingtimestyle = teachingtimestyle;
    }
    
    @XmlAttribute
    public String getTeachingtimestylecode()
    {
        return teachingtimestylecode;
    }

    public void setTeachingtimestylecode(String teachingtimestylecode)
    {
        this.teachingtimestylecode = teachingtimestylecode;
    }

    @XmlAttribute
	public String getEnddate()
	{
		return enddate;
	}

	public void setEnddate(String enddate)
	{
		this.enddate = enddate;
	}
	
	@XmlAttribute
	public String getStartdate()
	{
		return startdate;
	}

	public void setStartdate(String startdate)
	{
		this.startdate = startdate;
	}
}
