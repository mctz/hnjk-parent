package com.hnjk.edu.roll.vo;

public class ClassesVo {

	private String classname ;//班级名称
	private String grade;//关联年级
	private String brSchool;//关联学习中心
	private String major;//关联专业
	private String classic;//关联层次
	private String teachingType;//教学形式
	private String studentNum;//班级学生人数
	private String classesMaster;//班主任--教师/用户名
	
	public String getClassname() {
		return classname;
	}
	public void setClassname(String classname) {
		this.classname = classname;
	}
	public String getGrade() {
		return grade;
	}
	public void setGrade(String grade) {
		this.grade = grade;
	}
	public String getBrSchool() {
		return brSchool;
	}
	public void setBrSchool(String brSchool) {
		this.brSchool = brSchool;
	}
	public String getMajor() {
		return major;
	}
	public void setMajor(String major) {
		this.major = major;
	}
	public String getClassic() {
		return classic;
	}
	public void setClassic(String classic) {
		this.classic = classic;
	}
	public String getTeachingType() {
		return teachingType;
	}
	public void setTeachingType(String teachingType) {
		this.teachingType = teachingType;
	}
	public String getStudentNum() {
		return studentNum;
	}
	public void setStudentNum(String studentNum) {
		this.studentNum = studentNum;
	}
	public String getClassesMaster() {
		return classesMaster;
	}
	public void setClassesMaster(String classesMaster) {
		this.classesMaster = classesMaster;
	}
	
}
