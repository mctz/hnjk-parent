package com.hnjk.core.foundation.utils;

/**
 * 转换为Json的实体.
 * <code>JsonModel</code><p>
 * 
 * @author： 广东学苑教育发展有限公司.
 * @since： 2010-3-31 下午08:20:09
 * @see 
 * @version 1.0
 */
public class JsonModel implements java.io.Serializable{

	private static final long serialVersionUID = 4143049739983122093L;
	
	private String name;
	
	private String key;
	
	private String value;
	
	public JsonModel(){
		
	}
	
	public JsonModel(String name,String key,String value){
		this.name = name;
		this.key = key;
		this.value = value;
	}

	public String getKey() {
		return key;
	}

	public void setKey(String key) {
		this.key = key;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}
	
	
	
	
}
