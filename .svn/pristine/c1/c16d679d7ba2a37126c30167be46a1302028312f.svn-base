#main {
	background: url(../default/images/login_new/loginbg.jpg) center top
		no-repeat;
	width: 1030px;
	height: 513px;
	margin: 0 auto;
	padding: 0;
}

#main .leftside {
	width: 600px;
	height: 340px;
	float: left;
	margin: 100px 0 0 0;;
	padding: 10px 0 0 30px;
	text-align: left;
}

#main .rightside {
	width: 300px;
	height: 340px;
	float: left;
	margin: 100px 0 0 50px;
	padding: 10px 0 0 0;
	text-align: left;
}

#main .rightside p {
	line-height: 34px;
}

#main .link {
	width: 1000px;
	height: 30px;
	float: left;
	margin: 0;;
	padding: 10px 30px 0 0;
	text-align: right;
	font-size: 12px;
	color: #517087;
}

#main .link a {
	color: #517087;
}

#main .link a:hover {
	color: #c63f09;
}

#main .footer {
	width: 1000px;
	height: 30px;
	float: left;
	margin: 0;;
	padding: 0;
	text-align: center;
	font-size: 12px;
	color: #84aac6;
}

#query_main {
	background: url(../default/images/login_new/loginbg12.jpg) center top
		no-repeat;
	width: 1030px;
	height: 1500px;
	margin: 0 auto;
	padding: 0;
}

#query_main .leftside {
	width: 460px;
	height: 1000px;
	float: left;
	margin: 100px 0 0 0;;
	padding: 10px 0 0 30px;
	text-align: left;
}

#query_main .rightside {
	width: 400px;
	height: 340px;
	float: left;
	margin: 100px 0 0 50px;
	padding: 10px 0 0 0;
	text-align: left;
	color: #8e8e8e;
}

#query_main .rightside p {
	line-height: 20px;
	padding: 0;
	margin: 3px 0;
}

#query_main .link {
	width: 1000px;
	height: 30px;
	float: left;
	margin: 0;;
	padding: 10px 30px 0 0;
	text-align: right;
	font-size: 12px;
	color: #517087;
}

#query_main .link a {
	color: #517087;
}

#query_main .link a:hover {
	color: #c63f09;
}

#query_main .footer {
	width: 1000px;
	height: 30px;
	float: left;
	margin: 0;;
	padding: 0;
	text-align: center;
	font-size: 12px;
	color: #84aac6;
}

#query_main .list01 {
	width: 100%;
	padding: 10px 8px 0 0;
	font-size: 14px;
	color: black;
	line-height: 20px;
}

/*����������ʽ*/
A {
	FONT-SIZE: 12px;
	COLOR: #757272;
	line-height: 20px;
}

A:link {
	COLOR: #757272;
	TEXT-DECORATION: none;
}

A:visited {
	COLOR: #757272;
	TEXT-DECORATION: none;
}

A:hover {
	COLOR: #c63f09;
	TEXT-DECORATION: underline;
}

A:active {
	COLOR: #757272;
	TEXT-DECORATION: none;
}

.title1 {
	FONT: 15px "Microsoft Yahei";
	COLOR: #0463b4;
	padding: 10px 0 0 0;
	margin: 0;
	border-bottom: 1px solid #eee;
}

.title2 {
	FONT: 20px "Microsoft Yahei";
	COLOR: #0463b4;
	padding: 5px 0 1px 0;
	margin: 0;
	border-bottom: 1px solid #b9baba;
}

.font14 {
	color: #6c6c6c;
	FONT: 14px "Microsoft Yahei";
	text-align: left;
}

.list01 {
	width: 600px;
	padding: 10px 8px 0 0;
	font-size: 14px;
	color: black;
}

.list01 ul {
	padding: 0px;
	margin: 0px;
	font-size: 14px;
}

.list01 li {
	line-height: 26px;
	list-style-position: inside;
	list-style-type: disc;
	font-size: 14px;
}

.list01 li a {
	color: black;
	font-size: 14px;
	TEXT-DECORATION: none;
}

.list01 li a:hover {
	color: #c63f09;
	font-size: 14px;
	TEXT-DECORATION: none;
}