package com.hnjk.edu.teaching.vo;

public class MajorInfo {

	private String majorName;
	private String picture4Textbook;
	private String picture4course;
	private String classicName;
	
	public String getMajorName() {
		return majorName;
	}
	public void setMajorName(String majorName) {
		this.majorName = majorName;
	}
	public String getPicture4Textbook() {
		return picture4Textbook;
	}
	public void setPicture4Textbook(String picture4Textbook) {
		this.picture4Textbook = picture4Textbook;
	}
	public String getPicture4course() {
		return picture4course;
	}
	public void setPicture4course(String picture4course) {
		this.picture4course = picture4course;
	}
	public String getClassicName() {
		return classicName;
	}
	public void setClassicName(String classicName) {
		this.classicName = classicName;
	}
	
}
