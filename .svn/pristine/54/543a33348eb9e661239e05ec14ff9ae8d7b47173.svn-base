package com.hnjk.core.exception;

/**
 * 远程服务调用异常.
 * @author hzg
 *
 */
public class RpcServiceException extends RuntimeException {

	
	private static final long serialVersionUID = -918272347074391142L;

	public RpcServiceException(String msg){
		super(msg);
	}
	
	public RpcServiceException(Throwable t){
		super(t);
	}
	
	public RpcServiceException(String msg,Throwable t){
		super(msg,t);
	}
	
}
