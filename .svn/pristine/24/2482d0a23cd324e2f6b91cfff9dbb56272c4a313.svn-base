	clear: both;
	width: 98%;
	padding: 0 0 5px 0;
	margin: 0 0 4px 0;
}

.plan h1 {
	margin: 1px;
	font-size: 12px;
	background: #fff url(images/plan.jpg) left bottom no-repeat;
	height: 28px;
	padding: 5px 0 0 30px;
	color: #fff;
	margin: 0;
	line-height: 22px;
}

.xdzn a {
	color: #373737;
	float: right;
	margin: 5px 0;
}

.xdzn a:hover {
	color: #5b717a;
}

/*常用功能 */
.common {
	clear: both;
	width: 98%;
	border: 0px solid #aec2c9;
	padding: 0 0 5px 0;
	margin: 0 0 4px 0;
}

.common h1 {
	margin: 1px;
	font-size: 12px;
	background: #fff url(images/common.jpg) left bottom no-repeat;
	height: 28px;
	padding: 5px 0 0 30px;
	color: #fff;
	margin: 0;
	line-height: 22px;
}

.common img {
	margin: 5px 18px;
}

.common a img {
	border: 1px solid #aec2c9;
}

.common a:hover img {
	border: 1px dashed #0099FF;
}

/*精品课程 */
.jpkc {
	clear: both;
	width: 65%;
