}

/*教学资源使用情况 */
.res {
	width: 32%;
	border: 0px solid #aec2c9;
	padding: 0 0 5px 0;
	margin: 3px 2px 4px 2px;
	float: left;
	height: 280px;
}

/*访问记录*/
.record {
	clear: both;
	width: 98%;
	padding: 0 0 5px 0;
}

.record p {
	margin: 0 0 0 20px;
}

.record h1 {
	margin: 1px;
	font-size: 12px;
	background: #fff url(images/location.jpg) left bottom no-repeat;
	height: 28px;
	padding: 5px 0 0 30px;
	color: #fff;
	margin: 0;
	line-height: 22px;
}