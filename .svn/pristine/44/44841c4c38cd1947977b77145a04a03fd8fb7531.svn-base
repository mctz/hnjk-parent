package com.hnjk.edu.teaching.jaxb.entity;

import java.util.Collection;

/**
 * 
 * 导出的信息摘要
 * @author luof 
 */
public class AchievementTip
{	
	
    private OperateInfo operateinfo ;
	

    private Collection<AchievementCourseTip> coursetips ;

    public AchievementTip()
    {

    }

    public AchievementTip(OperateInfo operateinfo,
            Collection<AchievementCourseTip> coursetips)
    {
        this.operateinfo = operateinfo;
        this.coursetips = coursetips;
    }

    public Collection<AchievementCourseTip> getCoursetips()
    {
        return coursetips;
    }

    public void setCoursetips(Collection<AchievementCourseTip> coursetips)
    {
        this.coursetips = coursetips;
    }
    
    
    public OperateInfo getOperateinfo()
    {
        return operateinfo;
    }

    public void setOperateinfo(OperateInfo operateinfo)
    {
        this.operateinfo = operateinfo;
    }

}
